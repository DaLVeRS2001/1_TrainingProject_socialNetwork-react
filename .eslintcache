 action.newMessageText\n\t\t\t\t}\n\n\t\t\t\treturn {\n\t\t\t\t\t...state,\n\t\t\t\t\tmessagesData: [...state.messagesData, newMessage],\n\t\t\t\t}\n\t\t\t}else{\n\t\t\t\treturn state\n\t\t\t}\n\n\t\tdefault:\n\t\t\treturn state;\n\t}\n\n\treturn state\n}\n\n\n//                                       ACTION CREATORS\nexport const sendUserMessage = (newMessageText) => ({type: SEND_MESSAGE, newMessageText})\n\n\n//                                           THUNKS\nexport const sendMessage = (newMessageText) => (dispatch) => {\n\tdispatch(reset('DialogMessageForm'))\n\tdispatch(sendUserMessage(newMessageText))\n}\n\n\nexport default dialogsReducer","C:\\Users\\VladB\\Desktop\\programmingFiles\\jsLesson\\#projects\\1.1-first-project\\new-react-project\\src\\Redux\\Store.js",[],"C:\\Users\\VladB\\Desktop\\programmingFiles\\jsLesson\\#projects\\1.1-first-project\\new-react-project\\src\\Redux\\store-redux.js",[],"C:\\Users\\VladB\\Desktop\\programmingFiles\\jsLesson\\#projects\\1.1-first-project\\new-react-project\\src\\components\\Profile\\Posts\\PostsContainer.jsx",["156"],"C:\\Users\\VladB\\Desktop\\programmingFiles\\jsLesson\\#projects\\1.1-first-project\\new-react-project\\src\\components\\Dialogs\\DialogsContainer.jsx",[],"C:\\Users\\VladB\\Desktop\\programmingFiles\\jsLesson\\#projects\\1.1-first-project\\new-react-project\\src\\components\\Dialogs\\Message\\MessagesContainer.jsx",["157","158"],"import React from \"react\";\nimport Messages from \"../Messages/Messages\";\nimport {sendMessageActionCreator, updateMessageTextActionCreator} from \"../../../Redux/dialogsReducer\";\nimport storeContext from \"../../../storeContext\";\n\nconst MessagesContainer = (props) => {\n\treturn(\n\t\t<storeContext.Consumer>\n\t\t\t{\n\n\t\t\t\tstore=> {\n\n\n\t\t\t\t\treturn <Messages  />\n\t\t\t\t}\n\n\t\t\t}\n\t\t</storeContext.Consumer>\n\t)\n}\n\n\nexport default MessagesContainer\n\n\n\n\n\n\n\n\n\n\n",["159","160"],"C:\\Users\\VladB\\Desktop\\programmingFiles\\jsLesson\\#pro